{
    "name": "depiedra/laravel-settings",
    "description": "Persistent settings in Laravel.",
    "homepage": "https://github.com/DePiedra/laravel-settings",
    "license": "proprietary",
    "authors": [
        {
            "name": "Erdem Aksu",
            "email": "eaksu94@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/DePiedra/laravel-settings/issues"
    },
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.2",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "laravel/laravel": "^6.2",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Depiedra\\LaravelSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depiedra\\LaravelSettings\\ServiceProvider"
            ],
            "aliases": {
                "Setting": "Depiedra\\LaravelSettings\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
